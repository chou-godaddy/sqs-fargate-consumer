      "builds": [
        {
          "type": "docker",
          "name": "sqs-fargate-consumer",
          "dockerName": "sqs-fargate-consumer",
          "generate-dockerfile": true,
          "baseImage": "alpine:latest",
          "dockerPlatform": "linux/amd64",
          "entryPoint": "internal/cmd/sqs-fargate-consumer/main.go",
          "files": {
            "build/docker/sqs-fargate-consumer": "/go/bin/sqs-fargate-consumer",
