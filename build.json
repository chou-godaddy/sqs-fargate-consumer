          },
          "steps": [
            {
              "type": "aws-deployment",
              "regions": ["us-west-2"]
            },
            {
              "type": "standard-fargate-deployment",
              "regions": ["us-west-2"],
              "soap_service": true,
              "hosted_zone": "fulfillment.domains.{env_dns}.aws.gdcld.net",
              "test_path": "internal/integration",
              "app_encryption": false,
              "fargate_min_containers": {
                "dev": "1",
                "test": "1",
                "ote": "1",
                "prod": "1"
              },
              "fargate_desired_containers": {
                "dev": "1",
                "test": "1",
                "ote": "1",
                "prod": "1"
              },
              "fargate_cpu": {
                "prod": "512"
              },
              "fargate_memory": {
                "prod": "1GB"
              },
              "fargate_custom_task_role": "sqs-fargate-consumer-FargateTaskRole",
              "skip_tests": true
            }
          ]
        }
      ]
